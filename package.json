{
  "name": "lifx-desktop-app",
  "version": "0.0.1",
  "main": "electron.js",
  "scripts": {
    "start": "gulp && electron .",
    "build": "gulp && electron-packager . LIFX--overwrite --icon='./assets/icons/icon.icns' --ignore='./assets'",
    "buildwin": "gulp && electron-packager . LIFX--overwrite --platform=win32 --arch=ia32 --icon='./assets/icons/icon.icns' --ignore='./assets'"
  },
  "author": "Levi Cole",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "electron": "~1.8.2",
    "electron-packager": "^11.2.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-babel": "^7.0.1",
    "gulp-clean-css": "^3.9.3",
    "gulp-concat": "^2.6.1",
    "gulp-imagemin": "^4.1.0",
    "gulp-requirejs-optimize": "^1.3.0",
    "gulp-sass": "^3.1.0",
    "gulp-size": "^3.0.0",
    "gulp-svgmin": "^1.2.4",
    "gulp-uglify-es": "^1.0.1",
    "gulp-util": "^3.0.8",
    "jquery": "^3.3.1",
    "lodash": "^4.17.5"
  },
  "dependencies": {}
}
